:: Overview
<<silently>>
    <<set _intEnt = Object.entries($intellect)>>
    <<set _psyEnt = Object.entries($psyche)>>
    <<set _fysEnt = Object.entries($fysique)>>
    <<set _motEnt = Object.entries($motorics)>>
    <<set _skilltbl = "">>
    <<for _i, _stat range $statNames>>
        <<set _skilltbl += "<tr><td>" + _intEnt[_i][0] + "</td><td class='num'>" + _intEnt[_i][1] + "</td><td>" + _psyEnt[_i][0] + "</td><td class='num'>"+ _psyEnt[_i][1] + "</td><td>" + _fysEnt[_i][0] + "</td><td class='num'>" + _fysEnt[_i][1] + "</td><td>" + _motEnt[_i][0] + "</td><td class='num'>" + _motEnt[_i][1] + "</td></tr>">>
    <</for>>
<</silently>>
<table class="skills">
    <tr>
        <th colspan="8">Skills</th>
    </tr>
    <tr>
        <td id='intStat'>INT</td>
        <td class='num'>$stats.int</td>
        <td id='psyStat'>PSY</td>
        <td class='num'>$stats.psy</td>
        <td id='fysStat'>FYS</td>
        <td class='num'>$stats.fys</td>
        <td id='motStat'>MOT</td>
        <td class='num'>$stats.mot</td>
    </tr>
    _skilltbl
</table>
